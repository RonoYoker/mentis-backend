{
    "family": "onyxlocal-celery-heavyjobs",
    "containerDefinitions": [
        {
            "name": "onyxlocal-celery-heavyjobs",
            "image": "805134175167.dkr.ecr.ap-south-1.amazonaws.com/prod-onyx:prod-abl-0.0.49",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "onyxlocal-celery-heavyjobs-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "command": [
                "/bin/bash",
                "-c",
                "/usr/bin/supervisord -n -c /etc/supervisord.d/onyxlocal_celery_heavy.conf"
            ],
            "environment": [
                {
                    "name": "CURR_ENV",
                    "value": "prod"
                },
                {
                    "name": "BANK_NAME",
                    "value": "ABL"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "dockerLabels": {
                "app": "onyxlocal",
                "env": "prod",
                "lob": "abl"
            },
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/onyxlocal-celery-heavyjobs",
                    "awslogs-region": "ap-south-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::805134175167:role/prod-onyx-ecs-role",
    "executionRoleArn": "arn:aws:iam::805134175167:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "6144",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
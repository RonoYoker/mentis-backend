{
    "family": "onyxlocal-celery-heavyjobs",
    "containerDefinitions": [
        {
            "name": "onyxlocal-celery-heavyjobs",
            "image": "519499429845.dkr.ecr.ap-south-1.amazonaws.com/prod-onyx:prod-ibl-0.1.98",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "onyxlocal-celery-heavyjobs-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "command": [
                "/bin/bash",
                "-c",
                "/usr/bin/supervisord -n -c /etc/supervisord.d/onyxlocal_celery_heavy.conf"
            ],
            "environment": [
                {
                    "name": "CURR_ENV",
                    "value": "prod"
                },
                {
                    "name": "BANK_NAME",
                    "value": "IBL"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "startTimeout": 120,
            "stopTimeout": 120,
            "dockerLabels": {
                "app": "onyxlocal",
                "env": "prod",
                "lob": "ibl"
            },
            "ulimits": [
                {
                    "name": "nofile",
                    "softLimit": 100000,
                    "hardLimit": 100000
                },
                {
                    "name": "nproc",
                    "softLimit": 50000,
                    "hardLimit": 50000
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/onyxlocal-celery-heavyjobs",
                    "awslogs-region": "ap-south-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::519499429845:role/prod-onyx-ecs-role",
    "executionRoleArn": "arn:aws:iam::519499429845:role/prod-ecs-task-execution-role",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "2048",
    "memory": "16384"
}